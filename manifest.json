{
  "name": "OnlyNewHistory",
  "version": "1.0",
  "description": "auto clear old history elements",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "storage",
    "history",
    "alarms"
  ],
  "browser_action": {
    "default_popup": "options.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}